@{
    var routevalues = Context.Request.Query.ToDictionary(key => key.Key, value => value.Value.ToString());

    routevalues["toExcelFile"] = "True";
}

<a class="btn btn-info" asp-action="Index" asp-all-route-data="routevalues" target="_blank"> <i class="fa fa-files-o"></i> @HtmlLocalizer["Export"]</a>